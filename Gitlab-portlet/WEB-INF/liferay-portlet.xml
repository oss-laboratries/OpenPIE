<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>GitLab</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/jquery-ui.css</header-portlet-css>
		<header-portlet-css>/css/ui.jqgrid.css</header-portlet-css>
		<header-portlet-javascript>/js/jquery-1.12.3.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.jqGrid.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/grid.locale-ja.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery-ui.min.js</header-portlet-javascript>
		<css-class-wrapper>GitLab-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>